{
  "name": "mall-analysis-poc",
  "version": "1.0.0",
  "description": "자사몰 첫 페이지 자동 진단 POC - 90초 자동 채점/근거/개선 제안",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "worker": "tsx src/worker/index.ts",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build"
  },
  "keywords": [
    "e-commerce",
    "analysis",
    "lighthouse",
    "firecrawl"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/static": "^7.0.4",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "bullmq": "^5.8.3",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "lighthouse": "^12.1.0",
    "node-vibrant": "^3.1.6",
    "openai": "^4.55.4",
    "pdfkit": "^0.15.0",
    "pg": "^8.12.0",
    "puppeteer": "^24.16.1",
    "sharp": "^0.33.4",
    "uuid": "^10.0.0",
    "yaml": "^2.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/node": "^20.14.11",
    "@types/pdfkit": "^0.13.4",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.57.0",
    "pino-pretty": "^13.1.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  }
